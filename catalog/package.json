  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "cross-env NODE_ENV=production node ./internals/scripts/analyze.js",
    "bundlewatch": "cd build && bundlewatch --normalize '^.+?(\\.\\w+)(?:\\.chunk)?\\.js$' '*.js'",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.js --color --progress",
    "build:clean": "rimraf ./build",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "node server",
    "start": "cross-env NODE_ENV=development webpack serve --config internals/webpack/webpack.dev.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "lint": "npm run lint:app && npm run lint:css",
    "lint:css": "echo TODO: lint styles",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:prettier": "prettier --check",
    "lint:app": "npm run lint:prettier -- \"./app\" && npm run lint:eslint -- \"./app/**/*.[jt]s?(x)\"",
    "pretest": "npm run test:clean",
    "test": "npm run test:only -- --coverage",
    "test:clean": "rimraf ./coverage",
    "test:only": "cross-env NODE_ENV=test jest",
    "test:watch": "npm run test:only -- --watch",
    "prettify": "prettier --write",
    "gql:generate": "graphql-codegen",
    "prepare": "cd .. && husky install catalog/.husky"
  },
  "lint-staged": {
    "app/**/*.[jt]s?(x)": [
      "npm run lint:prettier --",
      "npm run lint:eslint --"
    ]
  },
